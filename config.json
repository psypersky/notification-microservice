{
  "env:global": {
    "port": 8080,
    "dbConnection": {
      "host": "localhost",
      "port": 5432,
      "database": "notifications",
      "user": "notificator",
      "max": 10,
      "idleTimeoutMillis": 30000
    },
    "engine": {
      "deadlockTimeout": 1800000,
      "workerPingInterval": 1000,
      "controllerCheckInterval": 1000,
      "loadBalancerInterval": 20,
      "sqsPollingInterval": 1000
    },
    "transports": {
      "email": {
        "AWSAccessKeyID": "key",
        "AWSSecretKey": "secret",
        "region": "us-east-1",
        "rateLimit": 5
      }
    },
    "AWS": {
      "SQS": {
        "BounceURL": "url",
        "ComplaintsURL": "url"
      }
    }
  }
}
